{
	"Basic Template": {
		"prefix": "template",
		"body": [
			"<!DOCTYPE html>",
			"<html lang=\"en\" dir=\"ltr\">",
			"<head>",
			"\t<meta charset=\"UTF-8\">",
			"\t<meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">",
			"\t<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">",
			"\t<meta name=\"description\" content=\"Description of the page\">",
			"\t<title>Document</title>",
			"\t<link rel=\"stylesheet\" href=\"https://cdn.jsdelivr.net/gh/programistazpolski/pzplui@master/style.min.css\" crossorigin=\"anonymous\">",
			"</head>",
			"<body class=\"pzplui-strict-mode\">",
			"\t<div class=\"text-center\">",
			"\t\t<h1 class=\"d1\">HELLO WORLD!</h1><br>",
			"\t\t<p>Edit your HTML file to make changes!</p>",
			"\t</div>",
			"</body>",
			"</html>"
		],
		"description": "A basic HTML5 Template for pzplUI"
	},
	"Alert": {
		"prefix": "alert",
		"body": "<div class=\"alert alert-${1:normal}>Hello World</div>",
		"description": "An alert"
	},
	"Accordion": {
		"prefix": "accordion",
		"body": [
			"<details class=\"accordion\">",
			"\t<summary>Summary Text</summary>",
			"\t<p>content</p>",
			"</details>"
		],
		"description": "An accordion"
	},
	"Badge": {
		"prefix": "badge",
		"body": "<span class=\"badge badge-${1:normal}\">Badge</span>",
		"description": "A badge"
	},
	"Pill Badge": {
		"prefix": "pill",
		"body": "<span class=\"badge badge-pill badge-${1:normal}\">Badge</span>",
		"description": "A badge"
	},
	"Box": {
		"prefix": "box",
		"body": [
			"<div class=\"box\">",
			"\t<p>content</p>",
			"</div>"
		],
		"description": "A badge"
	},
	"Button": {
		"prefix": "button",
		"body": "<button class=\"btn btn-${1:normal}\">Content</button>",
		"description": "A button"
	},
	"Card": {
		"prefix": "card",
		"body": [
			"<div class=\"card\" style=\"width: 18rem;\">",
			"\t<img src=\"./myPhoto.png\" class=\"img-fluid\">",
			"\t",
			"\t<div class=\"card-body\">",
			"\t\t<h5>Card title</h5>",
			"\t\t<p>Lorem ipsum dolor sit amet consectetur adipisicing elit. Fuga facere consectetur officiis possimus veritatis ratione.</p>",
			"\t\t<br><a href=\"#\" class=\"btn btn-normal\">Go somewhere</a>",
			"\t</div>",
			"</div>"
		],
		"description": "A card"
	},
	"Hero": {
		"prefix": "hero",
		"body": [
			"<div class=\"hero ${1:bg-success} consended\">",
			"\t<h1 class=\"title\">Hello World</h1>",
			"\t<h2 class=\"Subtitle\">Lorem ipsum</h2>",
			"</div>"
		],
		"description": "A hero component"
	},
	"Modal": {
		"prefix": "modal",
		"body": [
			"<div class=\"modal text-dark\" id=\"${1:myModal}\">",
			"\t<div class=\"modal-content\">",
			"\t\t<div class=\"modal-header consended\">",
			"\t\t\t<h5 class=\"modal-title\">Modal title</h5>",
			"\t\t</div>",
			"\t\t<div class=\"modal-body\">",
			"\t\t\t<p>Hello!</p>",
			"\t\t</div>",
			"\t\t<div class=\"modal-footer\">",
			"\t\t\t<button type=\"button\" class=\"btn btn-secondary\">Close</button>",
			"\t\t\t<button type=\"button\" class=\"btn btn-normal\">Save changes</button>",
			"\t\t</div>",
			"\t</div>",
			"</div>"
		],
		"description": "A modal"
	},
