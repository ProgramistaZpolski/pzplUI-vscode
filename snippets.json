	"Navbar": {
		"prefix": "navbar",
		"body": [
			"<nav class=\"navbar bg-dark text-white\">",
			"\t<a class=\"navbar-brand text-white\" href=\"javascript:void(0)\">Company</a>",
			"\t<ul>",
			"\t\t<li>",
